{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Scopes",
	"patterns": [
        { "include": "#block_comment" },
        { "include": "#block_string" },
        { "include": "#strings" },
        { "include": "#keywords" },
        { "include": "#types" },
        { "include": "#variables" },
        { "include": "#constants" },
        { "include": "#operators" },
        { "include": "#builtin_functions" },
        { "include": "#symbol" }
	],
	"repository": {
		"keywords": {
			"patterns": [
                {
                    "name": "keyword.control.builtin.scopes",
                    "match": "(quote|do(-in)?|if|elseif|else|return|let|del|fn|label|(raw)?call|syntax-extend|syntax-log)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "keyword.control.library.scopes",
                    "match": "(fn\\.\\.\\.|method|loop|repeat|capture|struct|enum|typefn|while|break|continue|for|in|match|from|using|filter|\\:|global|try|except|locals|import|defer|assert|define(((-block)?-scope)?-macro|-infix[\\<\\>])?)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "keyword.control.glsl.scopes",
                    "match": "(uniform|buffer|in|out|inout|varying|xvar|coherent|volatile|restrict|readonly|writeonly)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                }
            ]
		},
		"variables": {
			"patterns": [
                {
                    "name": "constant.language.builtins.scopes",
                    "match": "(debug-build\\?|compiler-(path|timestamp|dir)|syntax-scope|type-kind-(extern|function|integer|real|pointer|vector|array|tuple|union|typename))(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "constant.language.library.scopes",
                    "match": "(package|main-module\\?|module-(dir|path))(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "constant.language.glsl.scopes",
                    "match": "(gl_(Position|VertexID|FragCoord|FragDepth))(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                }
            ]
		},
		"constants": {
			"patterns": [
                {
				    "name": "constant.language.boolean.scopes",
				    "match": "(true|false)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
			    },
                {
				    "name": "constant.language.nothing.scopes",
				    "match": "(none|unnamed|null)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
			    },
                {
                    "match": "(\\'[^\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#]+)",
                    "name": "constant.character.symbol.scopes"
                },
                {
                    "comment": "Floating point literal (fraction)",
                    "name": "constant.numeric.float.scopes",
                    "match": "[\\-\\+]?[0-9][0-9_]*\\.[0-9][0-9_]*([eE][+-]?[0-9_]+)?(\\:f32|\\:f64)?(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "comment": "Floating point literal (exponent)",
                    "name": "constant.numeric.float.scopes",
                    "match": "[\\-\\+]?[0-9][0-9_]*(\\.[0-9][0-9_]*)?[eE][+-]?[0-9_]+(\\:f32|\\:f64)?(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "comment": "Floating point literal (typed)",
                    "name": "constant.numeric.float.scopes",
                    "match": "[\\-\\+]?[0-9][0-9_]*(\\.[0-9][0-9_]*)?([eE][+-]?[0-9_]+)?(\\:f32|\\:f64)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "comment": "Integer literal (decimal)",
                    "name": "constant.numeric.integer.decimal.scopes",
                    "match": "[\\-\\+]?[0-9][0-9_]*(\\:[ui](8|16|32|64|128|s|size))?(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "comment": "Integer literal (hexadecimal)",
                    "name": "constant.numeric.integer.hexadecimal.scopes",
                    "match": "[\\-\\+]?0x[a-fA-F0-9_]+(\\:[ui](8|16|32|64|128|s|size))?(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "comment": "Integer literal (octal)",
                    "name": "constant.numeric.integer.octal.scopes",
                    "match": "[\\-\\+]?0o[0-7_]+(\\:[ui](8|16|32|64|128|s|size))?(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "comment": "Integer literal (binary)",
                    "name": "constant.numeric.integer.binary.scopes",
                    "match": "[\\-\\+]?0b[01_]+(\\:[ui](8|16|32|64|128|s|size))?(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                }
            ]
		},
		"types": {
            "patterns":[
                {
                    "name": "support.type.builtin.scopes",
                    "match": "(void|bool|usize|u8|u16|u32|u64|i8|i16|i32|i64|f32|f64|type|list|string|integer|real|pointer|array|vector|tuple|union|typename|function|immutable|(Sampled)?Image|Sampler|Symbol|Unknown|Label|Closure|Frame|Any|Parameter|Syntax|Nothing|Scope|Anchor|Builtin|Exception|ReturnLabel|extern)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.type.library.scopes",
                    "match": "(Generator|Capture|Macro|rawstring|reference|fnchain|hash)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.type.contextual.scopes",
                    "match": "(this-struct)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.type.glm.scopes",
                    "match": "((i|u|d|b)?vec(2|3|4)|(i|u|d|b)?mat(2|3|4)(x(2|3|4))?)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.type.glsl.scopes",
                    "match": "(sampler|(i|u|g)?sampler(1D|2D|3D|Cube|2DRect|(1D|2D|Cube)Array|Buffer|2DMS(Array)?))(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                }
            ]
		},
		"builtin_functions": {
			"patterns": [
                {
                    "name": "support.function.builtin.scopes",
				    "match": "(pow|mix|step|smoothstep|floor|ceil|fract|(f|s)(abs|sign)|round|roundeven|trunc|fma|frexp|ldexp|(inverse)?sqrt|exp(2)?|log(2)?|radians|degrees|sin|cos|tan|asin|acos|atan|atan2|cross|normalize|length|distance|scopeof|offsetof|Image-(read|write)|sizeof|sample|mutable\\?|type-name|superof|string-match\\?|(file|directory)\\?|exit|compiler-version|load-library|prompt|countof|static-alloc|malloc(-array)?|free|alloca(-array|of)?|eval|list-load|globals|((mutable-)?pointer|integer|real|function|tuple|ReturnLabel|array|vector|typename|extern|(Sampled)?Image)-type|dump|dump-(label|list|frame)|Any-(extract-constant|repr|wrap)|branch|string-join|getelementptr|(volatile-)?(load|store)|storageof|import-c|(type|Scope)\\@|ptrtoint|inttoptr|(insert|extract)(value|element)|shufflevector|unconst(-all)?|compile(-glsl|-spirv|-object)?|typify|purify|(constant|signed)\\?|va(-keys|-values|-countof|\\@)|type-kind|nullof|undef|(type|bitcount)of|icmp[\\=\\!]\\=|icmp[\\<\\>]\\=?[su]|fcmp[\\=\\!]\\=[ou]|fcmp[\\<\\>]\\=?[ou]|bitcast|(fp|i)trunc|([sz]|fp)ext|fpto[su]i|[su]itofp|b(and|x?or)|f?add|f?sub|f?mul|[suf](div|rem)|shl|[al]shr)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.function.library.scopes",
				    "match": "(_|new|local|static|delete|sign|abs|(unroll-)?range|fold|map|zip|enumerate|unpack|(tuple|vector|array)of|unknownof|va-(empty\\?|types|join|each|each-reversed|key)|imply|forward-imply|forward-as|xpcall|require-from|char|getattr|(de)?cons|slice|(scope-|block-scope-)?macro|print|min|max|clamp|(tie|cond)-const|repr|(none|symbol|list|empty|type|integer|real|pointer|array|tuple|typename|Symbol|function-pointer)\\?|(integer|real|pointer|function|array|tuple|function-pointer|typename)-type\\?)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.function.sfx.builtin.scopes",
				    "match": "((enter-solver-cli|set-execution-mode|io-write|set-anchor|set-globals|set-typename(-storage|-super|-fields)|compiler-error|abort|discard|unreachable|set-signal-abort|(set|delete)-(type|scope)-symbol)!)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.function.sfx.library.scopes",
				    "match": "(((syntax-)?error|todo|raise)!)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.function.glsl.scopes",
				    "match": "(texelFetch|local_size|imageLoad|imageStore|(i|u)?image(1D|2D|3D|Cube|2DRect|(1D|2D|Cube)Array|Buffer|2DMS(Array)?))(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                },
                {
                    "name": "support.function.glm.scopes",
				    "match": "(dot|transpose)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
                }
            ]
		},
		"operators": {
            "patterns":[{
                "name": "keyword.operator.scopes",
                "match": "(as|\\<\\:|and|or|not|\\/\\/|\\@|[\\=\\!\\<\\>]\\=?|\\*\\*|[\\.\\?\\+\\-\\*\\/\\%\\&\\|\\^\\~]|\\<\\<|\\>\\>|\\.\\.)(?=[\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#])"
            }]
		},
		"symbol": {
			"patterns": [
				{
					"match": "([^\\s\\(\\)\\[\\]\\{\\}\\,\\;\\#]+)",
					"name": "meta.symbol.scopes"
				}
			]
		},
