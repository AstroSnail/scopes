
echo "generating core..." \
    && ./build_reference.sc > module-core.rst \
    && echo "generating glm..." \
    && ./build_reference.sc glm > module-glm.rst \
    && echo "generating Array..." \
    && ./build_reference.sc Array > module-Array.rst \
    && echo "generating Capture..." \
    && ./build_reference.sc Capture > module-Capture.rst \
    && echo "generating FunctionChain..." \
    && ./build_reference.sc FunctionChain > module-FunctionChain.rst \
    && echo "generating glsl..." \
    && ./build_reference.sc glsl > module-glsl.rst \
    && echo "generating itertools..." \
    && ./build_reference.sc itertools > module-itertools.rst \
    && echo "generating Map..." \
    && ./build_reference.sc Map > module-Map.rst \
    && echo "generating spicetools..." \
    && ./build_reference.sc spicetools > module-spicetools.rst \
    && echo "generating testing..." \
    && ./build_reference.sc testing > module-testing.rst \
    && make clean && make html

